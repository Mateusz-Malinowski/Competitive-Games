{
  "name": "web-games",
  "version": "1.0.0",
  "description": "web-games",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev-server": "concurrently --kill-others \"tsc --watch\" \"nodemon dist/server/index.js\"",
    "dev-client": "parcel watch src/client/**/index.html --dist-dir dist/client",
    "dev": "concurrently --kill-others \"npm run dev-server\" \"npm run dev-client\"",
    "prebuild": "rimraf dist",
    "build-server": "tsc",
    "build-client": "parcel build src/client/**/index.html --dist-dir dist/client --no-source-maps",
    "build": "npm run build-server && npm run build-client",
    "start": "node dist/server/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VmalinaV/Web-Games.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/VmalinaV/Web-Games/issues"
  },
  "homepage": "https://github.com/VmalinaV/Web-Games#readme",
  "dependencies": {
    "colors": "^1.4.0",
    "express": "^4.17.1",
    "ws": "^8.2.0"
  },
  "devDependencies": {
    "@parcel/transformer-image": "^2.0.0-rc.0",
    "@parcel/transformer-sass": "^2.0.0-rc.0",
    "@types/express": "^4.17.13",
    "@types/ws": "^7.4.7",
    "concurrently": "^6.2.1",
    "nodemon": "^2.0.12",
    "parcel": "^2.0.0-rc.0",
    "rimraf": "^3.0.2"
  }
}
